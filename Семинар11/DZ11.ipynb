vershiny = []
i = -20
di=0.01
lastdf = -54*i**2+10*i-20736*np.sin(i)*np.cos(np.cos(i))+10

while i < 20:
    i+=di    
    nextdf = -54*i**2+10*i-20736*np.sin(i)*np.cos(np.cos(i))+10

    if nextdf*lastdf<0:
        vershiny.append(i)
      
    lastdf = nextdf

for el in vershiny:
